} from 'react-redux';\n\nimport routes from '../../routes';\nimport Button from '../../components/Button/Button';\nimport { getProducts } from '../../redux/product/product-selector';\nimport productActions from '../../redux/product/product-action';\n\nimport { Container, ProductColor, SelectContainer } from './ProductDetailsPage.style';\n\nconst ProductDetailsPage = ({ match, history }) => {\n  const {\n    params: { productId },\n  } = match;\n\n  const dispatch = useDispatch();\n  const products = useSelector(getProducts);\n  const [product, setProduct] = useState(() =>\n    products.find(prod => prod.id === productId),\n  );\n  const [prise, setPrise] = useState(product?.price);\n  const [something, setSomething] = useState(0);\n  const [amount, setAmount] = useState(1);\n\n  useEffect(() => {\n    if (!product) {\n      history.push(routes.home);\n    }\n  }, []);\n\n  const addSomething = ({ target }) => {\n    setSomething(Number(target.value));\n    setPrise((product.price + Number(target.value)) * amount);\n  };\n\n  const isClearPrice = () => {\n    setPrise(product.price);\n    setAmount(1);\n    setSomething(0);\n  };\n\n  const isSubtract = () => {\n    if (amount === 1) return;\n    setPrise(prise - prise / amount);\n    setAmount(amount - 1);\n  };\n\n  const isAddAmount = () => {\n    setPrise(prise + prise / amount);\n    setAmount(amount + 1);\n  };\n\n  const isAddToCard = () => {\n    const productId = something ? `${product.id}/${something}` : product.id;\n    dispatch(\n      productActions.addToCard({\n        ...product,\n        id: productId,\n        amount: amount,\n        price: prise / amount,\n      }),\n    );\n  };\n\n  return (\n    <>\n      <h2>ProductDetailsPage</h2>\n      <Container>\n        <ProductColor color={product?.color}>\n          Color: {product?.color}\n          <p>Price: {product?.price}$</p>\n        </ProductColor>\n        <div>\n          <p>Price: {prise}$</p>\n          <SelectContainer>\n            <select name=\"something\" value={something} onChange={addSomething}>\n              <option value=\"0\">0$</option>\n              <option value=\"10\">Small 10$</option>\n              <option value=\"20\">Medium 20$</option>\n              <option value=\"30\">Large 30$</option>\n            </select>\n            <button onClick={isClearPrice}>Clear</button>\n          </SelectContainer>\n          <div>\n            <button onClick={isSubtract} disabled={amount < 2 ? true : false}>\n              -1\n            </button>\n            <span>{amount}</span>\n            <button onClick={isAddAmount}>+1</button>\n          </div>\n        </div>\n      </Container>\n      <Button text=\"Add to Card\" onClick={isAddToCard}></Button>\n    </>\n  );\n};\n\nexport default ProductDetailsPage;\n","D:\\yulia\\react\\test\\my-shop\\src\\components\\Header\\Header.jsx",[],"D:\\yulia\\react\\test\\my-shop\\src\\pages\\ProductGalleryPage\\ProductGalleryPage.jsx",[],"D:\\yulia\\react\\test\\my-shop\\src\\pages\\ProductGalleryPage\\ProductGalleryPage.style.js",[],"D:\\yulia\\react\\test\\my-shop\\src\\components\\CardList\\CardItem\\CardItem.jsx",[],"D:\\yulia\\react\\test\\my-shop\\src\\pages\\CardPage\\CardPage.jsx",["139"],"D:\\yulia\\react\\test\\my-shop\\src\\pages\\CardPage\\CardPage.style.js",[],"D:\\yulia\\react\\test\\my-shop\\src\\components\\UserForm\\UserForm.jsx",[],"D:\\yulia\\react\\test\\my-shop\\src\\components\\UserForm\\UserForm.style.js",[],"D:\\yulia\\react\\test\\my-shop\\src\\pages\\ProductGalleryPage\\Product\\Product.style.js",[],"D:\\yulia\\react\\test\\my-shop\\src\\pages\\ProductGalleryPage\\Product\\Product.jsx",[],"D:\\yulia\\react\\test\\my-shop\\src\\redux\\product\\product-action.js",["140"],"D:\\yulia\\react\\test\\my-shop\\src\\components\\App\\App.jsx",[],"D:\\yulia\\react\\test\\my-shop\\src\\js\\createRandomPrice.js",[],"D:\\yulia\\react\\test\\my-shop\\src\\js\\createRandomRGB.js",[],"D:\\yulia\\react\\test\\my-shop\\src\\routes.js",["141"],"D: